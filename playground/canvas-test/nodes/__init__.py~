import sys
import time
import canvas
from IDs import MessageList as ml

def main():

	sender = canvas.init_sender()
	if sys.argv[0] == "status":
		message_id = "001"
		message_data = "GET_STATUS_ALL"
		canvas.send(sender, message_id, message_data)
	if sys.argv[0] == "move":
		message_id = "001"
		message_data = "MOVE_FORWARD"
		canvas.send(sender, message_id, message_data)

if __name__ == '__main__':
    main()
